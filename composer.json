{
    "name": "marcmorente/pgn-parser",
    "description": "A PHP parser for the chess game notation format - PGN(Portable Game Notation)",
    "keywords": ["chess","pgn", "parser"],
    "homepage": "https://github.com/marcmorente/pgn-parser",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Marc Morente",
            "email": "marc.morente@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PGNParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PGNParser\\Test\\": "tests/"
        }
    }
}
